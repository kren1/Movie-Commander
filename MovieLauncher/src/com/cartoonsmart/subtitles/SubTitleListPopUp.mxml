         xmlns:s="library://ns.adobe.com/flex/spark" 
         xmlns:mx="library://ns.adobe.com/flex/mx" close="{PopUpManager.removePopUp(this)}">
  <fx:Script>
    <![CDATA[
      import mx.collections.ArrayCollection;
      import mx.managers.PopUpManager;
      
      [Bindable]
      public var dp:ArrayCollection;
    ]]>
  </fx:Script>
  <fx:Declarations>
    <!-- Place non-visual elements (e.g., services, value objects) here -->
  </fx:Declarations>
  <s:List id="list" itemRenderer="com.cartoonsmart.subtitles.SubtitlesListrenderer" dataProvider="{dp}" height="300" verticalScrollPolicy="on" width="400" />
</s:TitleWindow>
