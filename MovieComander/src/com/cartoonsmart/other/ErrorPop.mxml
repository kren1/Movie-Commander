<?xml version="1.0" encoding="utf-8"?>
<s:SkinnablePopUpContainer xmlns:fx="http://ns.adobe.com/mxml/2009"
                           xmlns:s="library://ns.adobe.com/flex/spark"
                           width="400" height="200" skinClass="slike.myPopUpSkin">
  <fx:Declarations>
    <!-- Place non-visual elements (e.g., services, value objects) here -->
  </fx:Declarations>
  <s:Button bottom="10" width="150" label="{resourceManager.getString('resources','CLOSE')}" horizontalCenter="0" click="{this.close()}"/>
  <s:Label left="10" top="10" text="{resourceManager.getString('resources','ERROR')}"  lineBreak="toFit"/>
  <s:Label id="ErrorMsg" left="10" top="50"/>
</s:SkinnablePopUpContainer>
